					<div class="user-menu">
						<div class="user-name"><span><img src="<%= user.thumbnail %>" alt=""></span>Hi, <%= user.username %>!</div>
						<ul>
							<li><a href="dashboard"><i class="sl sl-icon-settings"></i> Dashboard</a></li>
							<li><a href="dashboard-messages"><i class="sl sl-icon-envelope-open"></i> Messages</a></li>
